.col-lg-4.left
  .panel.panel-primary
    .panel-heading
      %h3.panel-title Pricetimate
    .panel-body
      =link_to image_tag('pricetimate-clip.png', class: 'pic'), 'http://www.pricetimate.com'
      %hr
      %h4 Designed to Help Artists Price Their Work
      =link_to 'Live Demo', 'http://pricetimate.com', class: 'btn btn-warning'
      =link_to 'http://github.com/andrewhouse/weekly', class: 'btn btn-info' do 
        Github 
      %button.btn.btn-success{'data-toggle' => 'modal', 'data-target' => '#pricetimate'} Preview
.modal.fade.pricetimate#pricetimate
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        %h1 Pricetimate
      .modal-body
        #priceCar.carousel.slide{'data-ride' => 'carousel'}
          .carousel-inner
            .item.active
              =image_tag('pricetimate2.png')
              .carousel-caption
                %h3
                  Items List
            .item
              =image_tag('pricetimate3.png')
              .carousel-caption
                %h3
                  Item Page
            .item
              =image_tag('pricetimate4.png')
              .carousel-caption
                %h3
                  Item Creation Page
            .item
              =image_tag('pricetimate5.png')
              .carousel-caption
                %h3
                  Artist Profile Page
            .item
              =image_tag('pricetimate.png')
              .carousel-caption
                %h3
                  Home Page
          %a.left.carousel-control{'href' => '#priceCar', 'role' => 'button', 'data-slide' => 'prev'}
            %span.control-left.fa-stack.fa-lg
              %i.fa.fa-chevron-left.fa-2x
          %a.right.carousel-control{'href' => '#priceCar', 'role' => 'button', 'data-slide' => 'next'}
